{
  "name": "notmuch-electron",
  "version": "5.1.0",
  "description": "not much frontend developped using Angular 7 with Electron (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/barais/notmuch-electron",
  "author": {
    "name": "Olivier Barais",
    "email": "olivier.barais@gmail.com"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meltedspark/electron-angular-native"
  },
  "main": "serve/main.electron.js",
  "keywords": [
    "mail",
    "mua",
    "notmuch",
    "angular",
    "angular 7",
    "electron",
    "typescript",
    "sass"
  ],
  "scripts": {
    "postinstall": "yarn electron:build:native",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ts-mocha -p e2e/tsconfig.e2e.json e2e/**/*.e2e-spec.ts",
    "start": "yarn run postinstall:electron && rimraf serve && yarn electron:build:serve && concurrently \"yarn ng:serve\"  \"yarn electron:serve\"",
    "start:prod": "rimraf serve && yarn electron:build:serve && concurrently \"yarn ng:watch:prod\" \"electron . --serve\"",
    "build": "yarn run postinstall:electron && yarn ng:build:prod && yarn electron:build:dist",
    "build:dev": "yarn ng:build:dev && yarn electron:build:dist",
    "electron:serve": "wait-on http-get://localhost:4200/ &&  electron . --serve",
    "electron:build:native": "electron-rebuild && rimraf build bin",
    "electron:build:serve": "tsc src/main.electron.ts --types --outDir serve",
    "electron:build:dist": "tsc src/main.electron.ts --types --outDir dist",
    "ng:build:prod": "ng build --prod --output-path=dist/",
    "ng:build:dev": "ng build --output-path=dist/",
    "ng:watch:prod": "ng build --prod --watch --delete-output-path=false --output-path=serve/",
    "ng:watch:dev": "ng build --watch --delete-output-path=false --output-path=serve/",
    "ng:serve": "ng serve --watch",
    "dist": "yarn build && yarn clean-build && npx electron-builder build --publish never",
    "dist:dev": "yarn build:dev && yarn clean-build && npx electron-builder build --publish never",
    "dist:windows:32": "cross-env ARCH=ia32 PLATFORM=win32 yarn build && yarn clean-build && npx electron-builder build --windows --ia32 --publish never",
    "dist:windows:64": "cross-env ARCH=x64 PLATFORM=win32 yarn build && yarn clean-build && npx electron-builder build --windows --x64 --publish never",
    "dist:linux:32": "cross-env ARCH=ia32 PLATFORM=linux yarn build && yarn clean-build && npx electron-builder build --linux --ia32 --publish never",
    "dist:linux:64": "cross-env ARCH=x64 PLATFORM=linux yarn build && yarn clean-build && npx electron-builder build --linux --x64 --publish never",
    "dist:osx": "cross-env ARCH=x64 PLATFORM=mac yarn build && yarn clean-build && npx electron-builder build --mac --publish never",
    "clean-build": "rimraf build-artifacts build"
  },
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular2-hotkeys": "^2.1.4",
    "core-js": "^2.6.1",
    "electron": "^9.4.0",
    "electron-context-menu": "^0.11.0",
    "electron-editor-context-menu": "^1.1.1",
    "electron-log": "^3.0.4",
    "electron-spellchecker": "^2.0.0",
    "ffi-napi": "^2.4.5",
    "ical.js": "^1.3.0",
    "imap": "^0.8.19",
    "jquery": "^3.5.0",
    "jsel": "^1.1.6",
    "mousetrap": "^1.6.3",
    "ngx-clipboard": "^12.0.0",
    "ngx-electron": "^2.1.1",
    "nodemailer": "^6.4.16",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.0",
    "quill": "^1.3.7",
    "rtm-api": "^1.3.1",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/ffi-napi": "^2.4.1",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.6",
    "@types/webdriverio": "^4.13.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.4",
    "electron-reload": "^1.4.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mocha": "^6.0.2",
    "node-loader": "^0.6.0",
    "spectron": "^5.0.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.14.0",
    "typescript": "3.1",
    "wait-on": "^3.2.0"
  }
}
